@page
@model VoiceAgentCSharp.Pages.LogoutModel
@{
    Layout = "_Layout";
    ViewData["Title"] = Localizer["LogoutTitle"];
}

<div class="main-container" style="text-align: center; padding-top: 100px;">
    <h1>@Localizer["LogoutTitle"]</h1>
    <p>@Localizer["LogoutMessage"]</p>
    <a href="/Login" class="btn-primary" style="display: inline-block; margin-top: 20px; text-decoration: none;">@Localizer["LoginAgain"]</a>
</div>
